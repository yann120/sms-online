              <div class="col-sm-6 col-md-4 col-lg-4 col-xl-4 mix marketing planning">
                <div class="portfolio-item">
                  <div class="shot-item">
                  <a class="overlay lightbox" href="portfolio/img1.jpg">
                      
                      <%= image_tag("portfolio/img1.jpg", alt: "")%>
                      <i class="fa fa-plus item-icon" aria-hidden="true"></i>
                  </a>
                  
                  </div>               
                </div>
              </div>
              <div class="col-sm-6 col-md-4 col-lg-4 col-xl-4 mix planning">
                <div class="portfolio-item">
                  <div class="shot-item">
                    <a class="overlay lightbox" href="img/portfolio/img2.jpg">
                      
                      <%= image_tag("portfolio/img2.jpg", alt: "")%>
